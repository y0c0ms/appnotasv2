{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AppNotas",
  "version": "1.0.0",
  "identifier": "com.appnotas.sticky",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "AppNotas",
        "width": 450,
        "height": 600,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}