{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:app:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "dialog:allow-open",
    "dialog:default",
    "core:webview:allow-set-webview-zoom",
    "core:path:default",
    "autostart:default"
  ]
}